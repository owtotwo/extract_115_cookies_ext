{
    "name": "提取115.com的Cookies",
    "version": "0.1.0",
    "author": "owtotwo",
    "description": "登录115时自动保存cookies",
    "permissions": [
        "contextMenus",
        "storage",
        "cookies",
        "downloads",
        "*://*.115.com/"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "icons": {
        "16": "images/cookie_16.png",
        "32": "images/cookie_32.png",
        "48": "images/cookie_48.png",
        "128": "images/cookie_128.png"
    },
    "manifest_version": 2
}